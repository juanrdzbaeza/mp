package org.mp.sesion07;

/**
 * Interface "DataAccessObject". Read and write objects to disk.
 * 
 * @author Juan A. Rodríguez Baeza
 * @version 0.1 02/06/2015
 *
 */
 public interface DataAccessObject{
	/**
	 * Method "escribir" write an object to disk
	 * @param object object to write to disk
	 */
	public void escribir(Object object);
	/**
	 * Method "leer" read an object to disk
	 */
	public Object leer();

}